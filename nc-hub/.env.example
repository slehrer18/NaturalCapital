# Supabase Configuration (Optional - app works without it using localStorage)
# Get these from your Supabase project settings
VITE_SUPABASE_URL=your_supabase_project_url
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Anthropic API Key (for Research Agent)
# The agent will work in the deployed app since it uses the API directly
# If you want to use it locally, you'd need to set up a proxy
VITE_ANTHROPIC_API_KEY=your_api_key_here
